<!-- map.wxml -->
<view class="yi">请选定您约会的地点（初始地点）</view>
<map id="map" longitude="113.324520" latitude="23.099994" scale="10" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 200px;"></map>
<view class="jingli">请书写您的约会计划</view>
<view class="jingli">
  <textarea class="weui-textarea" placeholder="请输入文本" style="height: 15.3em" />
</view>
<view class="one">
    <button class="chuangjian" type="primary" bindtap="jumpToMyPage">提交并返回首页</button>
</view>
